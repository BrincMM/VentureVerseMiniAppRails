<h1>Dashboard</h1>
<div class="row mt-4">
  <div class="col-md-6">
    <div class="card text-bg-primary mb-3">
      <div class="card-body">
        <h5 class="card-title">Total Users</h5>
        <p class="card-text display-4"><%= @users_count %></p>
      </div>
    </div>
  </div>
  <div class="col-md-6">
    <div class="card text-bg-success mb-3">
      <div class="card-body">
        <h5 class="card-title">Total Waiting List</h5>
        <p class="card-text display-4"><%= @waiting_list_count %></p>
      </div>
    </div>
  </div>
</div>
