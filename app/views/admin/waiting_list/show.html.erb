<h1>Waiting List Detail</h1>
<div class="card">
  <div class="card-body">
    <table class="table table-bordered">
      <tbody>
        <% @waiting_list.attributes.each do |key, value| %>
          <tr>
            <th><%= key %></th>
            <td><%= value.to_s %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <%= link_to 'Back to List', admin_waiting_list_index_path, class: 'btn btn-secondary' %>
  </div>
</div>
